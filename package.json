{
  "name": "estelam-platform",
  "version": "1.0.0",
  "description": "پلتفرم مدیریت فروشندگان مبتنی بر نقشه",
  "main": "src/index.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "wrangler": "wrangler",
    "d1:create": "wrangler d1 create estelam",
    "d1:migrate": "wrangler d1 execute estelam --file=./database/schema.sql",
    "d1:backup": "wrangler d1 export estelam --output=backup.sql",
    "kv:create": "wrangler kv:namespace create ESTELAM_KV",
    "kv:create-preview": "wrangler kv:namespace create ESTELAM_KV --preview",
    "deploy": "wrangler deploy",
    "pages:deploy": "wrangler pages deploy .next",
    "import:csv": "node scripts/import-csv.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "mapbox-gl": "^3.1.2",
    "next": "^15.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "stream-chat": "^9.22.0",
    "stream-chat-react": "^13.9.0",
    "supertokens-auth-react": "^0.50.0",
    "supertokens-node": "^23.0.1",
    "tailwind-merge": "^2.5.5",
    "telegraf": "^4.16.3",
